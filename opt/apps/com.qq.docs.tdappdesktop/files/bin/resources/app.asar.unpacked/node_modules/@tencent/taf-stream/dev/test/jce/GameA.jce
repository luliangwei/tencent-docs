module MiniGame
{
    enum E_CMD_TYPE
    {
        E_CMD_LBS_REPORT            = 0,            // LBS信息上报
        E_CMD_WEATHRR               = 1,            // 请求天气信息
        E_CMD_STAT                  = 2,            // 上报统计信息
        E_CMD_CRASH_REPORT          = 3,            // crash上报
        E_CMD_QCUBE_INFO            = 4,            // 上报Q立方基础信息
        E_CMD_GET_GUID              = 5,            // 获取GUID,更新用户信息
        E_CMD_GET_IPLIST            = 6,            // 获取后台服务器IP列表
        E_CMD_GET_UPDATE            = 7,            // 发起更新浏览器请求
        E_CMD_DO_NOTHING            = 8,            // 啥都不做
        E_CMD_GET_SURVEY            = 9,            // 调查问卷
        E_CMD_GET_HDICON            = 10,           // 高清图标
        E_CMD_REPORT_DESK           = 11,           // 上报桌面数据
        E_CMD_REPORT_SETTING        = 12,           // 上报桌面设置
        E_CMD_GET_STARTPAGE         = 13,           // 拉取起始页
        E_CMD_GET_TEMP_DESKDATA     = 14,           // 拉取修改的临时桌面数据
        E_CMD_REPORT_MACHINE_INFO   = 15,           // 上报机器信息
        E_CMD_GET_FEEDBACK          = 16,           // 拉取问题反馈
        E_CMD_GET_KEYVALUE_CONFIG   = 17,           // 拉取KEYVALUE配置项
        E_CMD_REPORT_RESOURCE_VIDEO    = 18,        // 上报视频信息
        E_CMD_REPORT_RESOURCE_AUDIO    = 19,        // 上报音频信息
        E_CMD_REPORT_RESOURCE_PICTURE  = 20,        // 上报图片信息
        E_CMD_GET_APP_RECOMMAND        = 21,        // 拉取应用推荐
        E_CMD_REPORT_SCREEN_CAPTURE    = 22,        // 上报截图         废弃不用了，没有一个线上版本使用这个命令字
        E_CMD_GET_APP_UPDATE           = 23,        // 拉取应用更新, 应用盒子使用
        E_CMD_GET_EGG_UPDATE           = 24,        // 拉去彩蛋动画更新
        E_CMD_GET_THEME_UPDATE         = 25,        // 拉取主题的更新
        E_CMD_ACCOUNT_REPORT           = 26,        // 账号情况上报
    };

	struct TCoinDesc
	{
		0 optional int gameID;
		1 optional string strCoinDesc;
	};

	struct TItem
	{
		0 optional int ID;
		1 optional string sDesc;
		//2 optional MiniGameB::TGameMsgAction st;
	};

	struct TUserInfo
	{
		0 optional short                                        var_short;
        1 optional int                                          var_int32;
        2 optional string                                       var_string;
        3 optional long                                         var_long;
        4 optional bool                                         var_bool;
        5 optional unsigned short                               var_uint16;

		10 optional TCoinDesc                                   st1;
		11 optional TItem                                       st2;

		20 optional map<string, string>                         map1;
		21 optional map<string, TCoinDesc>                      map2;
		22 optional map<string, map<string, TCoinDesc> >        map3;
		23 optional map<int, vector<TCoinDesc> >                map4;
		24 optional map<string, vector<string> >                map5;
		25 optional map<string, vector<byte> >                  map6;
		26 optional map<string, map<string, vector<byte> > >    map7;

		30 optional vector<string>                              list1;
		31 optional vector<TCoinDesc>                           list2;
		32 optional vector<byte>                                list3;
		33 optional vector<vector<byte> >                       list4;
		34 optional vector<vector<TCoinDesc> >                  list5;
	};
}; 
